#include<iostream>
using namespace std;

class Base
 {
   public:
    void display ()
    {
     cout<<"Display of Base class:"<<endl;
    }
 };
class Derived: public Base
  {
   public:
   void show()
    {
     cout<<"Show of Derived class"<<endl;
    }
  };
int main()
{
   Derived obj;
    obj.display();
    obj.show();
}
